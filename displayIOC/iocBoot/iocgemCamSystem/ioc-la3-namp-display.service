[Unit]
Description=IOC for LA3 North Amp camera display 
Wants=network-online.target
After=network-online.target
StartLimitInterval=200
StartLimitBurst=5

[Service]
Type=forking
User=jqg93617
# Note that PrivateTmp is implied
WorkingDirectory= /opt/gemini/la3/north_amp/gemini-amplifier-cameras/displayIOC/iocBoot/iocgemCamSystem
ExecStart=/usr/bin/procServ -n ioc-amp-cams-n -i ^D^C -p /tmp/pid.txt -P 20018 ./st.cmd
PIDFile=/tmp/pid.txt
Restart=always
RestartSec=30

[Install]
WantedBy=multi-user.target
